@model MidTermNetCore.Models.Khoa

@{
    ViewData["Title"] = "Chi tiết";
}

<h1>Chi tiết</h1>

<div>
    <h4>Khoa</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Tên khoa
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TenKhoa)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.MaKhoa">Sửa</a> |
    <a asp-action="Index">Về danh sách khoa</a>
</div>
